{% extends 'baseadmin.html.twig' %}

{% block title %}Category{% endblock %}

{% block body %}
    <h1>Category</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ category.id }}</td>
            </tr>
            <tr>
                <th>Category</th>
                <td>{{ category.catName }}</td>
            </tr>
        </tbody>
    </table>
    
    <a href="{{ path('app_category_index') }}" class="btn btn-primary">Back to list</a>

{% endblock %}
